        {
            "_id": "YsE43P9EGijckkHP",
            "name": "Demolitions",
            "type": "skill",
            "img": "modules/starwarsd6-essentialcompanion-od6s/art/items/skillIcons/tec_demolitions.svg",
            "data": {
